{
  "name": "adamdgraham-blog-clean",
  "version": "1.0.0",
  "description": "Cleaner, Simpler version of the blog",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "main": "index.js",
  "scripts": {
    "start": "npm run production",
    "dev": "cp client/index.html static/index.html && NODE_ENV=development webpack && webpack-dev-server --content-base static/ --hot --inline --devtool inline-source-map --history-api-fallback --colors",
    "production": "rm -rf static/index.html && NODE_ENV=production webpack -p && NODE_ENV=production node server/index.js",
    "build": "NODE_ENV=production webpack --progress --color -p --config webpack.prod.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Adam D. Graham <adamdgraham@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "0.13.1",
    "babel-polyfill": "6.9.1",
    "babel-register": "6.9.0",
    "express": "4.14.0",
    "immutable": "3.8.1",
    "object-assign": "4.1.0",
    "react": "15.2.1",
    "react-dom": "15.2.1",
    "react-redux": "4.4.5",
    "react-router": "2.6.0",
    "redux": "3.5.2"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-core": "6.10.4",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "eslint": "3.1.1",
    "eslint-plugin-react": "5.2.2",
    "node-sass": "3.8.0",
    "react-hot-loader": "1.3.0",
    "sass-loader": "4.0.0",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  }
}
